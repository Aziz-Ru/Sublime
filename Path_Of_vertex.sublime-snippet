vector<int> Path_Of_Vertex(int x){
  vector<int>ans;
  ans.push_back(x);
  while(path[x]!=-1){
  ans.push_back(path[x]);
  x=path[x];
  
 }
 reverse(all(ans));
 return ans;
}

